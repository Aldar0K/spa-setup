{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withIndex": true,
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "index": "templates/component/index.ts",
        "style": "templates/component/TemplateName.module.scss"
      }
    },
    "page": {
      "path": "src/pages",
      "withIndex": true,
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "templates/page/TemplateName.tsx",
        "index": "templates/page/index.ts",
        "style": "templates/page/TemplateName.module.scss"
      }
    }
  }
}
